{
  "name": "cross-domain-safe-weakmap",
  "version": "1.0.29",
  "description": "Cross domain safe WeakMap.",
  "main": "index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "lint": "eslint --ext ts,tsx,js,jsx src/ test/",
    "karma": "cross-env NODE_ENV=test babel-node --plugins=transform-es2015-modules-commonjs ./node_modules/.bin/karma start",
    "babel": "babel src/ --out-dir lib",
    "webpack": "babel-node --plugins=transform-es2015-modules-commonjs ./node_modules/.bin/webpack --progress",
    "test": "npm run lint && npm run tsc && npm run karma",
    "tsc": "tsc",
    "build": "npm run test && npm run babel && npm run webpack && npm run build:types",
    "build:flow": "find . -type f -not -path './node_modules/*' -name '*.d.ts' -exec sh -c 'flowgen --add-flow-header $1 -o ${1%.*.*}.js.flow' _ '{}' \\;",
    "build:tsc": "tsc src/* --outDir lib --declaration --emitDeclarationOnly",
    "build:types": "npm run build:tsc && npm run build:flow",
    "release": "./publish.sh",
    "release:patch": "./publish.sh patch",
    "release:minor": "./publish.sh minor",
    "release:major": "./publish.sh major",
    "clean": "rimraf dist coverage lib",
    "reinstall": "rimraf node_modules && npm install",
    "debug": "cross-env NODE_ENV=debug"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/krakenjs/grumbler.git"
  },
  "keywords": [
    "template"
  ],
  "licenses": [
    {
      "type": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  ],
  "files": [
    "dist/",
    "lib/",
    "src/",
    "postinstall.js"
  ],
  "readmeFilename": "README.md",
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "flowgen": "^1.14.1",
    "grumbler-scripts": "^5.1.1-beta.2",
    "mocha": "^4",
    "ts-node": "^10.2.0",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "cross-domain-utils": "^2.0.0"
  }
}
